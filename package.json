{
  "name": "readme-assert",
  "version": "5.0.6",
  "engines": {
    "node": ">=4"
  },
  "description": "Run code blocks in your readme as test",
  "main": "lib/index.js",
  "jsnext:main": "src/index.js",
  "bin": "cli.js",
  "scripts": {
    "build": "babel src/. -d lib/. --ignore=spec.js",
    "test:readme": "babel-node src/cli.js -m ./src",
    "test:spec": "run-tests src/**/*.spec.js -r @babel/register",
    "test": "npm-run-all test:*",
    "prettier": "prettier 'src/**/*'",
    "prepublish": "npm run build"
  },
  "author": {
    "name": "Sigurd Fosseng",
    "email": "sigurd@fosseng.net",
    "url": "http://laat.io"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/laat/readme-assert.git"
  },
  "dependencies": {
    "@babel/core": "^7.0.0",
    "assert-simple-tap": "^2.0.0",
    "babel-plugin-transform-comment-to-assert": "^4.1.0",
    "babel-plugin-transform-rename-import": "^2.0.0",
    "docopt": "~0.6.2",
    "find-up": "^4.0.0",
    "gfm-code-blocks": "^1.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/node": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/register": "^7.4.4",
    "@types/babel__core": "^7.1.2",
    "@types/docopt": "~0.6.31",
    "@types/eslint": "4.16.6",
    "@types/find-up": "^2.1.1",
    "@types/prettier": "^1.16.4",
    "eslint": "5.16.0",
    "eslint-config-airbnb-base": "13.1.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-import": "2.17.3",
    "npm-run-all": "4.1.5",
    "prettier": "^1.18.2",
    "run-tests": "1.0.4"
  },
  "files": [
    "lib",
    "cli.js"
  ]
}
